<!DOCTYPE html>
<html lang="en">
    <%- include('head.ejs'); %>
<body>
<header class="section">
    <div class="container">
        <div class="categories" id="cats">
            <% cats.map(function(e){ %>
                <a href="/chi-tiet/<%=e.id %>">
                    <div class="box">
                        <div class="img">
                            <img src="<%= e.image %>"/>
                        </div>
                        <p><%= e.name %>></p>
                    </div>
                </a>
            <% }) %>
        </div>
    </div>
</header>


</body>
</html>